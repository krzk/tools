#!/bin/sh
# SPDX-License-Identifier: GPL-2.0

set -x

[ -d /sys/kernel/config/usb_gadget ] || exit 0

cd /sys/kernel/config/usb_gadget

cd g2

echo -n "" > UDC || true

rm configs/c.1/ecm.usb0

rmdir configs/c.1/strings/0x409
rmdir configs/c.1

rmdir functions/ecm.usb0
rmdir strings/0x409

cd ..
rmdir g2
